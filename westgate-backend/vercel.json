{
  "version": 2,
  "name": "westgate-backend-api",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "echo 'No build step required for Node.js server'",
  "outputDirectory": ".",
  "framework": null
}
